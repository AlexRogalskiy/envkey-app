      #preloader
        .contents
          / img.logo src="data:image/svg+xml;base64,"

          img.spinner src="data:image/svg+xml;base64,"

    #wrap

    script src="/openpgp.min.js"

    - if build?
      script src="/javascripts/index.js"
    - else
      script src="http://localhost:8080/index.js"
